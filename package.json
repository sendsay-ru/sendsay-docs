{
  "name": "sendsay-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "docusaurus build",
    "clear": "docusaurus clear",
    "deploy": "./bin/deploy.sh",
    "docusaurus": "docusaurus",
    "e2e": "yarn playwright test",
    "eslint": "eslint src/ ",
    "eslint:fix": "yarn eslint --fix",
    "fix": "node scripts/translate && yarn lint:fix",
    "lint": "yarn pretify && yarn eslint",
    "lint:fix": "yarn pretify:fix && yarn eslint:fix",
    "pretify": "prettier -c src config scripts *.js **/*.md **/*.mdx",
    "pretify:fix": "yarn pretify -w",
    "serve": "docusaurus serve",
    "start": "docusaurus start",
    "start:en": "docusaurus start --locale en",
    "swiz": "docusaurus swizzle @docusaurus/theme-classic --typescript",
    "swizzle": "docusaurus swizzle",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations --locale en"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@algolia/client-search": "^4.9.1",
    "@docusaurus/core": "3.0.1",
    "@docusaurus/plugin-client-redirects": "3.0.1",
    "@docusaurus/preset-classic": "3.0.1",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "docusaurus-lunr-search": "^3.3.0",
    "docusaurus-plugin-image-zoom": "^1.0.1",
    "docusaurus-plugin-yandex-metrica": "^1.1.0",
    "glob": "^8.0.3",
    "prism-react-renderer": "^1.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.0.1",
    "@docusaurus/types": "^3.0.1",
    "@playwright/test": "^1.39.0",
    "@docusaurus/tsconfig": "^3.0.1",
    "@types/node": "^20.8.10",
    "@types/react": "18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "chalk": "^4.1.2",
    "dotenv": "16.3.1",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "prettier": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.10"
  }
}
