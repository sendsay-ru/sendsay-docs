{
  "name": "sendsay-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "start:en": "docusaurus start --locale en",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "./bin/deploy.sh",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations --locale en",
    "write-heading-ids": "docusaurus write-heading-ids",
    "pretify": "prettier -c src *.js **/*.md **/*.mdx",
    "lint": "yarn pretify && yarn eslint",
    "eslint": "eslint src/ ",
    "pretify:fix": "yarn pretify -w",
    "eslint:fix": "yarn eslint --fix",
    "lint:fix": "yarn pretify:fix && yarn eslint:fix",
    "fix": "node scripts/translate && yarn lint:fix"
  },
  "dependencies": {
    "@algolia/client-search": "^4.9.1",
    "@docusaurus/core": "2.2.0",
    "@docusaurus/plugin-client-redirects": "2.2.0",
    "@docusaurus/preset-classic": "2.2.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "docusaurus-lunr-search": "^2.1.15",
    "glob": "^8.0.3",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.2.0",
    "@docusaurus/types": "^2.2.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/react": "17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "prettier": "^2.6.2",
    "typescript": "^4.7.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^16.17.0 || >=18.0.0",
    "yarn": ">=1.22.10"
  }
